		{
			"name": "Login Utente",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Verifica che la risposta contenga un token",
							"if (pm.response.code === 200) {",
							"    // Parla di JWT nel body della risposta o header, adatta il percorso in base alla tua API",
							"    var jsonData = pm.response.json();",
							"",
							"    // Salva il token JWT in una variabile d'ambiente chiamata 'jwt_token'",
							"    pm.environment.set(\"jwt_token\", jsonData.token);",
							"    ",
							"    // Stampa nel console log per verificare che il token sia stato salvato",
							"    console.log(\"JWT salvato:\", jsonData.token);",
							"    console.log(jsonData); // Stampa il contenuto dell'intera risposta JSON",
							"} else {",
							"    console.log(\"Login fallito, impossibile salvare il token\");",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
